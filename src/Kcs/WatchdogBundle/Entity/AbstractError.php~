<?php

namespace Kcs\WatchdogBundle\Entity;

class AbstractError
{
    /**
     * @var integer
     */
    protected $id;

    /**
     * @var integer
     */
    protected $level;

    /**
     * @var string
     */
    protected $message;

    /**
     * @var string
     */
    protected $file;

    /**
     * @var integer
     */
    protected $line;

    /**
     * @var array|mixed
     */
    protected $trace;

    /**
     * @var array|mixed
     */
    protected $variables;

    /**
     * @var array|mixed
     */
    protected $user;


}